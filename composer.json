{
  "name": "vouchsafe/vouchsafe-php",
  "type": "library",
  "license": "MIT",
  "description": "Official PHP library for the Vouchsafe identity platform. Use it for easy KYC checks, identity verification, remote right-to-work and more.",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://openapi-generator.tech",
  "authors": [
    {
      "name": "OpenAPI",
      "homepage": "https://openapi-generator.tech"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.0",
    "guzzlehttp/psr7": "^1.7 || ^2.0",
    "psr/simple-cache": "^1 || ^2 || ^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Vouchsafe\\": "src/",
      "Vouchsafe\\OpenAPI\\": "src/openapi/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vouchsafe\\Test\\": "test/"
    }
  }
}
